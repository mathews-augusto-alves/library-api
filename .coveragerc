[run]
source = src
omit = 
    */tests/*
    */__pycache__/*
    */migrations/*
    */venv/*
    */env/*
    */node_modules/*
    */\.*/*
    */build/*
    */dist/*
    */coverage/*
    */htmlcov/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # type: ignore
    # noqa
    # fmt: off
    # fmt: on

[html]
directory = htmlcov
title = API Library - Cobertura de Testes 